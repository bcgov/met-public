{
  "makefile.extensionOutputFolder": "./.vscode",
  "python.defaultInterpreterPath": "${workspaceFolder}/met-api/venv/bin/python",
  "python.envFile": "${workspaceFolder}/met-api/.env",
  "python.analysis.extraPaths": [
    "./analytics-api/src",
    "./met-cron/src",
    "./met-api/src"
  ],
  "pylint.args": ["--load-plugins", "pylint_flask_sqlalchemy", "pylint_flask"],
  "python.analysis.autoImportCompletions": true,
  "python-envs.pythonProjects": [],
  "sonarlint.connectedMode.project": {
    "connectionId": "bcgov-sonarcloud",
    "projectKey": "bcgov_met-public"
  },
  "python-envs.defaultEnvManager": "ms-python.python:venv",
  "helm-intellisense.customValueFileNames": ["values_dev.yaml", "values.yaml"],
  "jest.jestCommandLine": "npm test --",
  "jest.rootPath": "met-web/"
}
